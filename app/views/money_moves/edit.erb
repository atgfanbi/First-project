<%= form_for @move do |f| %>
    <% if @move.errors.any? %>
        <div class="error">
          <h3>Errors:</h3>
          <% @move.errors.full_messages.each do |msg| %>
              <div><%= msg %></div>
          <% end %>
        </div>
    <% end %>
    <p>
      <%= f.label :price %><br />
      <%= f.text_field :price %>
    </p>
    <p>
      <%= f.label :description %><br />
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.label :date %><br />
      <%= f.datetime_select :date %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', category_path(@move.category_id) %>