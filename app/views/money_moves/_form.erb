<%= form_for([@category, @category.money_moves.build]) do |f| %>
    <p>
      <%= f.label :price %><br />
      <%= f.text_field :price %>
    </p>
    <p>
      <%= f.label :description %><br />
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.label :date %><br />
      <%= f.datetime_select :date %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>